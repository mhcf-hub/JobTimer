<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jobtimer">

    <application
        android:name=".Variables"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:installLocation="preferExternal"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".EditTimingActivity"></activity>
        <activity android:name=".SingleTimingActivity" />
        <activity android:name=".NotesActivity" />
        <activity android:name=".OverviewJobsActivity" />
        <activity android:name=".CreateNewJobActivity" />
        <activity android:name=".TimingsActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />

</manifest>